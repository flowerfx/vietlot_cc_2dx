<?xml version ='1.0' encoding='UTF-8'?>
<GAME>
	<!-- root template is the template that all widget will load it although define or note -->
	<root_template>
		<template id="0" name="temp_root">
			<common_value>				
				<common    		name="anchor_point"	value="0.5_0.5"/>	
				<common			name="use_atlas"   	value="1"/>
			</common_value>
		</template>
	</root_template>
	<!-- default template is the template that use in all res screen -->
	<default_template>
		<template id="0" name="temp_text_default">
			<common_value>				
				<common    		name="font_idx"			value="0"/>	
				<common			name="bitmap_font"   	value="1"/>
			</common_value>
		</template>
		<template id="6" name="temp_scale9_custom">
			<common_value>
				<common name="scale_9" 			value="1"/>
				<common name="use_nine_path" 	value="1"/>
				<common name="stage_scale9" 	value="3"/>
			</common_value>
		</template>
		<template id="20" name="temp_btn_blue">
			<common_value>
				<common 		name="source" 			value="default-round-blue.9.png"/>
				<common 		name="press" 			value="default-round-blue.9.png"/>
				<common 		name="disable" 			value="button_dis.9.png"/>
				<common			name="scale_9"			value="1"/>
				<common			name="use_nine_path" 	value="1"/>
				<common 		name="scale_press" 		value="-0.05"/>
			</common_value>
		</template>
		<template id="21" name="temp_btn_yellow">
			<common_value>
				<common 		name="source" 			value="default-round-yellow.9.png"/>
				<common 		name="press" 			value="default-round-yellow.9.png"/>
				<common 		name="disable" 			value="button_dis.9.png"/>
				<common			name="scale_9"			value="1"/>
				<common			name="use_nine_path" 	value="1"/>
				<common 		name="scale_press" 		value="-0.05"/>
			</common_value>
		</template>
		<template id="22" name="temp_btn_red">
			<common_value>
				<common 		name="source" 			value="default-round.9.png"/>
				<common 		name="press" 			value="default-round.9.png"/>
				<common 		name="disable" 			value="button_dis.9.png"/>
				<common			name="scale_9"			value="1"/>
				<common			name="use_nine_path" 	value="1"/>
				<common 		name="scale_press" 		value="-0.05"/>
			</common_value>
		</template>
	</default_template>
	
	
	<!-- list template is the template that use in specific res screen -->
	<template_list range_size_x="10_-1" 	design_size="960_640">
		<template id="0" name="temp_text_0">
			<template_value>				
				<template    		name="temp_text_default"/>	
			</template_value>
			<size 		origin = "25_25" 	/>
		</template>
		<template id="0" name="temp_text_0_small">
			<template_value>				
				<template    		name="temp_text_default"/>	
			</template_value>
			<size 		origin = "20_20" 	/>
		</template>
		<template id="0" name="temp_text_0_big">
			<template_value>				
				<template    		name="temp_text_default"/>	
			</template_value>
			<size 		origin = "30_30" 	/>
		</template>
		
	</template_list>
	
	
	
</GAME>